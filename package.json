{
  "name": "react-typescript-express-ssr-boilerplate",
  "version": "1.0.0",
  "description": "A starter for a project using TypeScript ExpressJs and ReactJs with server side rendering.",
  "scripts": {
    "analyze": "source-map-explorer dist/bundle.*",
    "start": "cross-env NODE_ENV=development TARGET=client nodemon",
    "server": "cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "build:client": "cross-env NODE_ENV=production TARGET=client webpack",
    "build:server": "cross-env NODE_ENV=production TARGET=server webpack",
    "build": "run-s build:client build:server",
    "prebuild": "rimraf dist",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "cross-env NODE_ENV=production build-storybook",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weyheyhey/react-typescript-express-ssr-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "react",
    "express",
    "storybook",
    "webpack",
    "css-modules",
    "redux",
    "ssr"
  ],
  "author": "weyheyhey",
  "license": "Unlicense",
